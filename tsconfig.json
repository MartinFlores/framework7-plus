{
    "compilerOptions": {
      "target": "ES6",
      "lib": ["DOM", "ESNext"],
      "module": "ESNext",
      "moduleResolution": "Node",
      "jsx": "react",
      "declaration": true,
      "outDir": "dist",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
  }
  